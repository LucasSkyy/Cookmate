{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "^/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "^/js/(.*)",
      "dest": "/public/js/$1"
    },
    {
      "src": "^/styles/(.*)",
      "dest": "/public/styles/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}